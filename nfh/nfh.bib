@article{ito_signal-responsive_2000,
	title = {Signal-responsive gating of porous membranes by polymer brushes},
	volume = {11},
	issn = {1099-1581},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/1099-1581%28200003%2911%3A3%3C136%3A%3AAID-PAT961%3E3.0.CO%3B2-F},
