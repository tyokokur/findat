	keywords = {intelligent material, permeability, polymer brush, porous membrane, signal-responsiveness},
	file = {Ito_Soon Park_2000_Signal-responsive gating of porous membranes by polymer brushes.pdf:G\:\\My Drive\\Zotero_sync\\Ito_Soon Park_2000_Signal-responsive gating of porous membranes by polymer brushes.pdf:application/pdf;Snapshot:C\:\\Users\\takas\\Zotero\\storage\\98IJQRMH\\1099-1581(200003)113136AID-PAT9613.0.html:text/html},
