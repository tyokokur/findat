	date = {2018-07-04},
	langid = {english},
	keywords = {Dipolar chains, Molecular dynamic simulations, Polymer brushes, Self-consistent field, Structure of polymers},
	file = {Birshtein et al_2018_How to fold back grafted.pdf:G\:\\My Drive\\Zotero_sync\\Birshtein et al_2018_How to fold back grafted.pdf:application/pdf},
